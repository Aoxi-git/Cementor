FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U.fluid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    y1 
    {
        type            fixedValue;
        value           uniform (2 0 0);
    }



    y0 
    {
        type            fixedValue;
        value           uniform (-2 0 0);
    }




    x0 
    {
        type            fixedValue; 
	value           uniform (0 2 0); 
    
    }




    x1 
    {
        type            fixedValue; 
	value           uniform (0 -2 0); 
    
    }


    z1 
    {
        type            fixedValue;
	value           uniform (0 0 0); 
    
    }



    z0 
    {
        type            fixedValue; 
	value           uniform (0 0 0); 
    
    }


}

